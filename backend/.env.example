# Database Connection String (Supabase PostgreSQL)
ConnectionStrings__DefaultConnection=User Id=YOUR_USERID_HERE;Password=YOUR_PASSWORD_HERE;Server=aws-1-eu-central-1.pooler.supabase.com;Port=6543;Database=postgres

# JWT Configuration
Jwt__Key=YOUR-SUPER-SECRET-JWT-KEY-MINIMUM-32-CHARACTERS-LONG-FOR-SECURITY
Jwt__Issuer=B2BCommerce.Backend.API
Jwt__Audience=B2BCommerce.Backend.Client
Jwt__ExpirationMinutes=15
Jwt__RefreshTokenExpirationDays=7

# CORS Allowed Origins (comma-separated for multiple)
Cors__AllowedOrigins__0=http://localhost:3000
Cors__AllowedOrigins__1=http://localhost:3001

# ASP.NET Core Environment
ASPNETCORE_ENVIRONMENT=Development
